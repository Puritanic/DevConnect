{
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "mocha": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "no-console": 0
  },
  "globals": {
    "process": true
  }
}